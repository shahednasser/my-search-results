{
 "name": "Dolphin - Remember Your Search Results",
 "version": "1",
 "description": "Store, search and manage your search results.",
 "permissions": [
         "storage",
         "tabs",
         "webRequest",
         "*://*.google.com/*"
 ],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": true
  },
  "page_action": {
    "default_icon": {                    
        "16": "images/16x16.png",           
        "24": "images/24x24.png",           
        "32": "images/32x32.png"            
    },
    "default_title": "Dolphin",      
    "default_popup": "popup.html"        
  },
 "manifest_version": 2
}
