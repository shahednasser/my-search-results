{
 "name": "Dolphin - Remember Your Search Results",
 "version": "1",
 "description": "Store, search and manage your search results.",
 "permissions": [
         "storage",
         "tabs",
         "webRequest",
         "*://*.google.com/"
 ],
 "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": true
  },
 "manifest_version": 2
}
